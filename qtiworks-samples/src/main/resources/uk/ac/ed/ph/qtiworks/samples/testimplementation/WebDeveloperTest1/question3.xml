        <choiceInteraction responseIdentifier="RESPONSE" shuffle="true" maxChoices="1">
            <prompt>What code is required to display an image on your webpage?<br /><br /></prompt>
            <simpleChoice identifier="ChoiceA">&lt;img src = "myimage.jpg" /img src&gt;</simpleChoice>
            <simpleChoice identifier="ChoiceB">&lt;img import = "myimage.jpg" /&gt;</simpleChoice>
            <simpleChoice identifier="ChoiceC">&lt;img src = "myimage.jpg" /&gt;</simpleChoice>
            <simpleChoice identifier="ChoiceD">&lt;image source = "myimage.jpg" /&gt;</simpleChoice>
        </choiceInteraction>
    </itemBody>
